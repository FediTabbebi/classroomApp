{
  "hosting": {
    "public": "build/web",
    "rewrites": [
      {
        "source": "/**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=600, s-maxage=600"
          }
        ]
      }
    ]
  }
}